<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LetterSpacingTextView">
        <attr name="letterSpacing" format="float|reference" />
    </declare-styleable>
    <declare-styleable name="CircleGraphValueView">
        <attr name="titleColor" format="color|reference" />
        <attr name="status" format="string|reference" />
    </declare-styleable>
    <declare-styleable name="LoginImageView">
        <attr name="strokeColor" format="reference|color" />
        <attr name="symbolRes" format="integer|reference" />
        <attr name="strokeWidth" format="dimension|reference" />
    </declare-styleable>
    <declare-styleable name="CircleGraphView">
        <attr name="innerCircleColor" format="color|reference" />
        <attr name="outerCircleRadius" format="dimension|reference" />
        <attr name="innerCircleRadius" format="dimension|reference" />
        <attr name="outerStrokeColor" format="color|reference" />
        <attr name="innerStrokeColor" format="color|reference" />
        <attr name="outerStrokeWidth" format="dimension|reference" />
        <attr name="innerStrokeWidth" format="dimension|reference" />
    </declare-styleable>
    <declare-styleable name="BarChartView">
        <attr name="barWidth" format="dimension|reference" />
        <attr name="barColor" format="color|reference" />
        <attr name="barDistance" format="dimension|reference" />
        <attr name="underLineColor" format="color|reference" />
        <attr name="underLineWidth" format="dimension|reference" />
        <attr name="valueTextColor" format="color|reference" />
        <attr name="valueTextSize" format="dimension|reference" />
        <attr name="barMaxHeight" format="dimension|reference" />
        <attr name="barRadius" format="dimension|reference" />
        <attr name="bottomTextColor" format="color|reference" />
        <attr name="bottomTextSize" format="dimension|reference" />
    </declare-styleable>
</resources>